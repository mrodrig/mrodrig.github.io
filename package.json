{
  "name": "mrodrig-github-io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && npm run deploy:github",
    "deploy:github": "push-dir --dir=dist --branch=master --cleanup",
